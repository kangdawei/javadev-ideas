<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>


    <context id="DB2Tables" targetRuntime="MyBatis3">

        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver" userId="root" password="root"
                        connectionURL="jdbc:mysql://192.168.244.132:3306/jpdb?autoReconnect=true&amp;useUnicode=true&amp;characterEncoding=UTF-8">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <javaModelGenerator targetPackage="com.cmp.common.bean" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <sqlMapGenerator targetPackage="resources/mappers" targetProject="src/main">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.cmp.common.mappers" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        <!-- Tables -->
        <table tableName="t_goods" domainObjectName="GoodsBean" selectByExampleQueryId="false"
               enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
               enableCountByExample="false">
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table tableName="t_goods_detail" domainObjectName="GoodsDetailBean"
               selectByExampleQueryId="false" enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" enableCountByExample="false">
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table tableName="t_goods_brand" domainObjectName="GoodsBrandBean"
               selectByExampleQueryId="false" enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" enableCountByExample="false">
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table tableName="t_goods_imgs" domainObjectName="GoodsImgsBean"
               selectByExampleQueryId="false" enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" enableCountByExample="false">
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table tableName="t_goods_tag" domainObjectName="GoodsTagBean"
               selectByExampleQueryId="false" enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" enableCountByExample="false">
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table tableName="t_goods_type" domainObjectName="GoodsTypeBean"
               selectByExampleQueryId="false" enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" enableCountByExample="false">
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table tableName="t_index_data" domainObjectName="IndexDataBean"
               selectByExampleQueryId="false" enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" enableCountByExample="false">
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table tableName="t_index_data_def" domainObjectName="IndexDataDefBean"
               selectByExampleQueryId="false" enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" enableCountByExample="false">
            <property name="useActualColumnNames" value="true"/>
        </table>
    </context>
</generatorConfiguration>